ges":"91","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"46"},"/Users/lysong/Desktop/myReact_project/myProject /src/index.js",[],["92","93"],"/Users/lysong/Desktop/myReact_project/myProject /src/App.js",["94","95"],"import React, { useState, useEffect } from 'react';\nimport { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';\nimport Navbar from './components/Navbar';\nimport Signup from './Navbars/Signup';\nimport Signout from './Navbars/Signout';\nimport Homepage from './Navbars/Homepage';\nimport Contact from './Navbars/Contact';\nimport About from './Navbars/About';\nimport Login from './Navbars/login';\nimport NewsComponent from './News/LastestNews';\nimport { auth } from './firebaseConfig';\nimport { onAuthStateChanged } from 'firebase/auth';\nimport LastestNews from './News/LastestNews';\nimport TeslaNews from './News/TeslaNews';\nimport Sports from './News/AppleNews';\nimport AppleNews from './News/AppleNews';\nimport WallStreet from './News/WallStreet';\n\nfunction App() {\n  const [isAuthenticated, setIsAuthenticated] = useState(null);\n\n  useEffect(() => {\n    const unsubscribe = onAuthStateChanged(auth, (user) => {\n      setIsAuthenticated(!!user);\n    });\n\n    return () => unsubscribe();\n  }, []);\n\n  return (\n    <Router>\n      <Navbar />\n      <Switch>\n        <Route exact path=\"/\">\n          {isAuthenticated ?  <LastestNews/> : <Homepage />}\n        </Route>\n        <Route path=\"/home\">\n          {isAuthenticated ? <LastestNews /> : <Redirect to=\"/\" />}\n        </Route>\n        <Route path=\"/login\">\n          <Login />\n        </Route>\n        <Route path=\"/signUp\">\n          <Signup />\n        \n        </Route>\n        <Route path=\"/logout\">\n          <Signout />\n        </Route>\n        <Route path='/contact'>\n          <Contact />\n        </Route>\n        <Route path='/about'>\n          <About />\n        </Route>\n        <Route path='/lastest'>\n          <LastestNews />\n        </Route>\n        <Route path= '/tesla'>\n          { isAuthenticated ? <TeslaNews/ > : <Homepage/>}\n        </Route>\n        <Route path= '/apple'>\n          { isAuthenticated ? <AppleNews/ > : <Homepage/>}\n        </Route>\n        <Route path= '/wallstreet'>\n          { isAuthenticated ? <WallStreet/ > : <Homepage/>}\n        </Route>\n        <Redirect to=\"/\" /> {/* Redirects to Homepage if no routes match */}\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/lysong/Desktop/myReact_project/myProject /src/components/Navbar.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/firebaseConfig.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/components/LoginHomepage.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/ApiFetch/useFetchData.js",[],["96","97"],"/Users/lysong/Desktop/myReact_project/myProject /src/News/LastestNews.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/News/NewsArticle.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/News/TeslaNews.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/News/AppleNews.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/login.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/Signup.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/Signout.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/ShareForm.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/Homepage.js",["98"],"import React from 'react';\nimport myImage from '../assets/img/wikley-oh-169hero-news-shutterstock.webp';\nimport '../css/Homepage.css'; // Make sure the path to the CSS file is correct\nimport { Link } from 'react-router-dom/cjs/react-router-dom.min';\nimport NewsComponent from '../News/LastestNews';\nimport LoginHomepage from '../components/LoginHomepage';\n\nconst Homepage = () => {\n  return (\n    <div>\n    <div className=\"homepage-container\">\n      <div className=\"content-container\">\n        <h1>Discover the Best News Experience</h1>\n        <p>Stay Informed with Insight News</p>\n        <Link to=\"/signUp\"> <button className=\"get-started-btn\">Get Started</button> </Link>\n        <button className=\"read-more-btn\">Read More</button>\n      </div>\n      <div className=\"image-container\">\n        <img src={myImage} alt=\"News\" />\n      </div>\n\n    </div>\n    <NewsComponent />\n\n    </div>\n    \n  );\n};\n\nexport default Homepage;","/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/About.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/Contact.js",["99","100","101"],"import React, {useState, useEffect} from 'react';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport {auth, db} from '../firebaseConfig'\nimport { signInWithEmailAndPassword } from 'firebase/auth';\nimport { useHistory } from 'react-router-dom'; // Import useHistory\nimport { getFirestore, collection, addDoc, query, where, getDocs, doc, updateDoc } from 'firebase/firestore';\nimport ContactForm from './ContactForm';\nimport { arrayUnion } from 'firebase/firestore';\n\nconst Contact = () => {\n\n  const [isSubmitted, setIsSubmitted ] = useState(false);\n  const history = useHistory(); \n\n  const handleFormSubmit = async ({ email, name, message }) => {\n    const contactsCollectionRef = collection(db, \"contacts\");\n    const q = query(contactsCollectionRef, where(\"email\", \"==\", email));\n  \n    try {\n      const querySnapshot = await getDocs(q);\n      let docRef;\n  \n      if (!querySnapshot.empty) {\n        // If the document exists, retrieve the document reference\n        const docSnapshot = querySnapshot.docs[0];\n        docRef = doc(db, \"contacts\", docSnapshot.id);\n        \n        // Push the new message into the existing array of messages\n        await updateDoc(docRef, {\n          messages: arrayUnion(message) // Use arrayUnion to add a message to the \"messages\" array\n        });\n      } else {\n        // If the document doesn't exist, create a new one with a messages array\n        docRef = await addDoc(contactsCollectionRef, {\n          email: email,\n          name: name,\n          messages: [message] // Start with the initial message in an array\n        });\n      }\n  \n      setIsSubmitted(true);\n    } catch (error) {\n      console.error(\"Error adding document: \", error);\n    }\n  };\n  \n  \n  \n\n\n  useEffect(() => {\n    if (isSubmitted) {\n      const timer = setTimeout(() => {\n        history.push('/home');\n      }, 2000);\n\n      return () => clearTimeout(timer); // Cleanup the timeout on component unmount\n    }\n  }, [isSubmitted, history]);\n\n  // Conditional rendering for the thank you message\n  if (isSubmitted) {\n    return (\n      <div className=\"alert alert-success\" role=\"alert\">\n        <h2>Thank you for your Contacting Us, We will look into that !</h2>\n      </div>\n    );\n  }\n\n\n \n  return (\n    <ContactForm onSubmit={handleFormSubmit} />\n  );\n};\n\n\nexport default Contact;\n","/Users/lysong/Desktop/myReact_project/myProject /src/News/WallStreet.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/News/GenericNews.js",[],"/Users/lysong/Desktop/myReact_project/myProject /src/News/Pagination.js",["102"],"import '../css/pagination.css'\n\nconst Pagination = ({ articlesPerPage, totalArticles, currentPage, paginate }) => {\n    const pageNumbers = [];\n\n    for (let i = 1; i <= Math.ceil(totalArticles / articlesPerPage); i++) {\n        pageNumbers.push(i);\n    }\n    return (\n        <nav>\n            <ul className='pagination'>\n                {pageNumbers.map(number => (\n                    <li key={number} className={`page-item ${number === currentPage ? 'active' : ''}`}>\n                        <a onClick={(e) => {\n                            e.preventDefault();  // Prevent default anchor behavior\n                            paginate(number);\n                        }} href=\"#\" className='page-link'>\n                            {number}\n                        </a>\n                    </li>\n                ))}\n            </ul>\n        </nav>\n    );\n};\n\n\n\nexport default Pagination","/Users/lysong/Desktop/myReact_project/myProject /src/Navbars/ContactForm.js",[],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":10,"column":8,"nodeType":"109","messageId":"110","endLine":10,"endColumn":21},{"ruleId":"107","severity":1,"message":"111","line":15,"column":8,"nodeType":"109","messageId":"110","endLine":15,"endColumn":14},{"ruleId":"103","replacedBy":"112"},{"ruleId":"105","replacedBy":"113"},{"ruleId":"107","severity":1,"message":"114","line":6,"column":8,"nodeType":"109","messageId":"110","endLine":6,"endColumn":21},{"ruleId":"107","severity":1,"message":"115","line":3,"column":9,"nodeType":"109","messageId":"110","endLine":3,"endColumn":13},{"ruleId":"107","severity":1,"message":"116","line":4,"column":10,"nodeType":"109","messageId":"110","endLine":4,"endColumn":36},{"ruleId":"107","severity":1,"message":"117","line":6,"column":10,"nodeType":"109","messageId":"110","endLine":6,"endColumn":22},{"ruleId":"118","severity":1,"message":"119","line":14,"column":25,"nodeType":"120","endLine":17,"endColumn":59},"no-native-reassign",["121"],"no-negated-in-lhs",["122"],"no-unused-vars","'NewsComponent' is defined but never used.","Identifier","unusedVar","'Sports' is defined but never used.",["121"],["122"],"'LoginHomepage' is defined but never used.","'auth' is defined but never used.","'signInWithEmailAndPassword' is defined but never used.","'getFirestore' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble